
struct Thing<T> {
    a: T;
}

fn create_Thing<T>(a: T) -> Thing<T> {
    return new Thing { a: a };
}

fn identity<T>(thing: T) -> T {
    return thing;
}

fn identity2<T>(thing: T) -> T {
    return identity(thing);
}


fn main() -> i32 {
    let a := 32;

    let thing := create_Thing(a);

    let b := identity2(a);

    return b;
}
